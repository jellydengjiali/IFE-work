{
  "name": "Test",
  "version": "1.0",
  "manifest_version": 2,
  "description": "The first extension that I made.",
  "icons": {"48": "icon.png",
            "128": "icon.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {"scripts":["background.js"]},
  "permissions": ["webRequest", "webRequestBlocking",
                  "*://www.baidu.com/*",
                  "http://*.google.com/*",
                  "https://*.google.com/*"]
}
